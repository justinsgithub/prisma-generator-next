{
  "name": "prisma-generator-next",
  "version": "0.0.1",
  "description": "scaffold NextJS api routes with middleware and zod validations using Prisma",
  "homepage": "https://github.com/justinsgithub/prisma-generator-next#README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinsgithub/prisma-generator-next"
  },
  "author": "Justin Angeles <justinaawd@gmail.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator",
    "middleware",
    "orm",
    "zod",
    "nextjs"
  ],
  "main": "dist/generator.js",
  "private": false,
  "license": "MIT",
  "bin": {
    "prisma-generator-next": "dist/generator.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/generator.js",
    "build": "tsup src/generator.ts --format cjs,esm --dts",
    "dev": "tsup src/generator.ts --format cjs,esm --dts --watch",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "@prisma/generator-helper": "^4.8.0",
    "@prisma/internals": "^4.8.0",
    "chalk": "^4.1.0",
    "pluralize": "^8.0.0",
    "prettier": "^2.8.1",
    "ts-morph": "^17.0.1",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/node": "17.0.21",
    "@types/pluralize": "^0.0.29",
    "@types/prettier": "2.4.2",
    "jest": "27.4.7",
    "prisma": "4.8.0",
    "ts-jest": "27.1.4",
    "tsup": "^6.6.3",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "next-api-middleware": "^2.0.1",
    "zod": "^3.21.0"
  }
}
